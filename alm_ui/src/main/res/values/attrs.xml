<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AlmEditText">
        <attr name="type" format="enum">
            <enum name="android_default" value="0" />
            <enum name="alphanumeric" value="1" />
            <enum name="number" value="2" />
            <enum name="number_positive" value="3" />
            <enum name="amount" value="4" />
            <enum name="amount_positive" value="5" />
            <enum name="alm_recycler_text" value="6" />
            <enum name="quantity" value="7" />
            <enum name="quantity_positive" value="8" />
        </attr>
    </declare-styleable>
</resources>